{
	"name": "input-test",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"main": "src/lib/server/server.ts",
	"scripts": {
		"build": "rolldown -c rolldown.config.ts",
		"check": "tsc --noEmit && biome check --write",
		"dev": "npm run build && npm run dev:build & npm run dev:server",
		"dev:build": "rolldown -w -c rolldown.config.ts",
		"dev:server": "node --watch-path ./dist --watch .",
		"format": "biome format --write",
		"preview": "npm run build && NODE_ENV=production node .",
		"start": "npm run build && node ."
	},
	"dependencies": {
		"@effect/cli": "^0.69.2",
		"@effect/platform": "^0.90.6",
		"@effect/platform-node": "^0.96.0",
		"@preact/signals": "^2.3.1",
		"effect": "^3.17.9",
		"preact": "^10.27.0",
		"preact-render-to-string": "^6.6.1",
		"urlpattern-polyfill": "^10.1.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@types/node": "^24.3.0",
		"lightningcss": "^1.30.1",
		"rolldown": "^1.0.0-beta.34",
		"rollup-plugin-esbuild": "^6.2.1",
		"typescript": "~5.8.3"
	}
}

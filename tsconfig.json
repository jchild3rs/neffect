{
	"compilerOptions": {
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
		"target": "ES2024",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"lib": ["ES2024", "DOM", "DOM.Iterable"],
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"moduleDetection": "force",
		"noEmit": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"baseUrl": "./",
		"paths": {
			"react": ["./node_modules/preact/compat/"],
			"react/jsx-runtime": ["./node_modules/preact/jsx-runtime"],
			"react-dom": ["./node_modules/preact/compat/"],
			"react-dom/*": ["./node_modules/preact/compat/*"],
			"neffect/link": ["./lib/router/link.tsx"],
			"neffect/use-*": ["./lib/router/use-*.ts"],
			"neffect/server": ["./lib/server/server.ts"],
			"neffect/document": ["./lib/server/_document.tsx"],
			"neffect/app": ["./lib/server/_app.tsx"],
			"neffect/server/*": ["./lib/server/*.ts"],
			"neffect/plugin": ["./lib/plugin.ts"]
		},
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedSideEffectImports": true,
		"exactOptionalPropertyTypes": true,
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"esModuleInterop": false,
		"types": [],
		"isolatedModules": true,
		"noImplicitReturns": true,
		"noErrorTruncation": false,
		"allowJs": false,
		"checkJs": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUncheckedIndexedAccess": false,
		"strictNullChecks": true,
		"incremental": true,
		"removeComments": false
	},
	"include": ["src", "lib", "app.config.ts"]
}
